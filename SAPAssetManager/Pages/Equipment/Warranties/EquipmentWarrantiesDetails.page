{
	"_Type": "Page",
	"_Name": "EquipmentWarrantiesDetailsPage",
	"Caption": "/SAPAssetManager/Rules/Equipment/Warranties/EquipmentWarrantiesDetailCaption.js",
	"Controls": [
		{
			"_Type": "Control.Type.SectionedTable",
			"_Name": "EquipmentWarrantiesDetailsSectionedTable",
			"Sections": [
				{
					"_Type": "Section.Type.KeyValue",
					"_Name": "EquipmentWarrantiesDetailsSection",
					"Header": {
						"Caption": "$(L,warranty_details)"
					},
					"KeyAndValues": [
						{
							"KeyName": "$(L,type)",
							"Value": "{WarrantyTypeDesc}"
