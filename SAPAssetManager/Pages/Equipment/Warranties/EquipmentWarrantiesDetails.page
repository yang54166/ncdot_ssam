						{
							"KeyName": "$(L,number)",
							"Value": "/SAPAssetManager/Rules/Equipment/Warranties/EquipmentWarrantiesNumber.js"
						},
						{
							"KeyName": "$(L,start_date)",
							"Value": "/SAPAssetManager/Rules/Equipment/Warranties/EquipmentWarrantiesStartDate.js"
						},
						{
							"KeyName": "$(L,end_date)",
							"Value": "/SAPAssetManager/Rules/Equipment/Warranties/EquipmentWarrantiesEndDate.js"
						},
						{
							"KeyName": "$(L,description)",
							"Value": "/SAPAssetManager/Rules/Equipment/Warranties/EquipmentWarrantiesDescription.js"
						}
					]
				},
				{
					"_Type": "Section.Type.KeyValue",
					"_Name": "EquipmentWarrantiesNotesSection",
					"Layout": {
						"NumberOfColumns": 1,
						"MinimumInteritemSpacing": "$(PLT,66,0)"
					},
					"KeyAndValues": [
						{
							"KeyName": "$(L, notes)",
							"Value": "/SAPAssetManager/Rules/Equipment/Warranties/EquipmentWarrantiesLongText.js"
