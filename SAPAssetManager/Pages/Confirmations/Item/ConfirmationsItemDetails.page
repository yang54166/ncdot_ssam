						},
						{
							"KeyName": "$(L, actual_duration)",
							"Value": "/SAPAssetManager/Rules/Confirmations/Item/GetItemDetailsDuration.js"
						},
						{
							"KeyName": "$(L, status)",
							"Value": "/SAPAssetManager/Rules/Confirmations/Item/GetItemDetailsStatus.js"
