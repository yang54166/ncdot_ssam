						{
							"KeyName": "$(L,category_4)",
							"Value": "/SAPAssetManager/Rules/ServiceOrders/Details/Category4Value.js"
						}
					],
					"_Type": "Section.Type.KeyValue",
					"_Name": "Categorization"
