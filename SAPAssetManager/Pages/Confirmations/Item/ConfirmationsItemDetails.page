						{
							"KeyName": "$(L,category_2)",
							"Value": "/SAPAssetManager/Rules/ServiceOrders/Details/Category2Value.js"
						},
						{
							"KeyName": "$(L,category_3)",
							"Value": "/SAPAssetManager/Rules/ServiceOrders/Details/Category3Value.js"
