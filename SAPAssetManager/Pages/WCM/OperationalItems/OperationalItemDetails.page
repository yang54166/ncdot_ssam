					"Layout": {
						"NumberOfColumns": 1
					},
					"SimplePropertyCells": [
						{
							"SimplePropertyCell": {
								"KeyName": "$(L, item_number)",
								"Value": "/SAPAssetManager/Rules/WCM/OperationalItems/Details/ItemNumber.js"
							}
						},
						{
							"SimplePropertyCell": {
								"KeyName": "$(L,item_category)",
