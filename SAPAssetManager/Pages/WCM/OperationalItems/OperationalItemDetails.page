						"Header": {
							"Caption": "$(L,documents)"
						},
						"Search": {
							"Enabled": false
						},
						"MaxItemCount": 2,
						"Footer": {
							"Caption": "$(L,see_all)",
							"AccessoryType": "disclosureIndicator",
							"FooterStyle": "attribute",
							"AttributeLabel": "/SAPAssetManager/Rules/Documents/Count/DocumentsBDSCount.js",
							"OnPress": "/SAPAssetManager/Actions/Documents/DocumentsBDSListNav.action",
