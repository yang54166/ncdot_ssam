	"RequestOptions": {
		"UpdateMode": "Replace"
	},
    "Headers": {
        "OfflineOData.TransactionID": "#Property:ObjectID"
    },
    "UpdateLinks": [
		{
			"Property": "Priority_Nav",
			"Target": {
				"EntitySet": "ServicePriorities",
				"QueryOptions": "/SAPAssetManager/Rules/ServiceOrders/CreateUpdate/ServiceRequestPriorityReadLink.js"
			}
		},
		{
			"Property": "Urgency_Nav",
			"Target": {
				"EntitySet": "ServiceUrgencySet",
				"QueryOptions": "/SAPAssetManager/Rules/ServiceOrders/CreateUpdate/ServiceRequestUrgencyReadLink.js"
			}
		},
		{
			"Property": "Impact_Nav",
			"Target": {
				"EntitySet": "ServiceImpactSet",
				"QueryOptions": "/SAPAssetManager/Rules/ServiceOrders/CreateUpdate/ServiceRequestImpactReadLink.js"
			}
		},
		{
			"Property": "Customer_Nav",
			"Target": {
				"EntitySet": "S4BusinessPartners",
				"ReadLink": "/SAPAssetManager/Rules/ServiceOrders/CreateUpdate/ServiceRequestSoldToPartyReadLink.js"
			}
		}
